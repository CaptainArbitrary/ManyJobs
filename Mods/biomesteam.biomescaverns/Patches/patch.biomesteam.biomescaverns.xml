<?xml version="1.0" encoding="UTF-8"?>
<Patch>

  <!-- #region MJobs_PriorityHauling -->

  <Operation Class="XmlExtensions.OptionalPatchExternal">
    <ModClass>ManyJobs.ManyJobs</ModClass>
    <field>MJobs_PriorityHauling</field>
    <caseTrue>

      <Operation Class="PatchOperationReplace">
        <xpath>Defs/WorkGiverDef[defName = "BMT_TakeMushroomWineOutOfFermentingBarrel"]/workType</xpath>
        <value>
          <workType>MJobs_PriorityHauling</workType>
        </value>
      </Operation>

      <Operation Class="PatchOperationReplace">
        <xpath>Defs/WorkGiverDef[defName = "BMT_FillMushroomFermentingBarrel"]/workType</xpath>
        <value>
          <workType>MJobs_PriorityHauling</workType>
        </value>
      </Operation>

    </caseTrue>
  </Operation>

  <!-- #endregion -->


</Patch>
