<?xml version="1.0" encoding="UTF-8"?>
<Patch>

  <Operation Class="ManyJobs.PatchOps.IfWorkTypeIsEnabled">
    <workType>MJobs_PriorityHauling</workType>
    <operations>

      <li Class="PatchOperationReplace">
        <xpath>Defs/WorkGiverDef[defName = "VFEI_InsertFirstGenome"]/workType</xpath>
        <value>
          <workType>MJobs_PriorityHauling</workType>
        </value>
      </li>

      <li Class="PatchOperationReplace">
        <xpath>Defs/WorkGiverDef[defName = "VFEI_InsertSecondGenome"]/workType</xpath>
        <value>
          <workType>MJobs_PriorityHauling</workType>
        </value>
      </li>

      <li Class="PatchOperationReplace">
        <xpath>Defs/WorkGiverDef[defName = "VFEI_InsertThirdGenome"]/workType</xpath>
        <value>
          <workType>MJobs_PriorityHauling</workType>
        </value>
      </li>

    </operations>
  </Operation>

</Patch>
