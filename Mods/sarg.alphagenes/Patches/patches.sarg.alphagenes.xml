<?xml version="1.0" encoding="UTF-8"?>
<Patch>

  <!-- #region MJobs_Caring -->

  <Operation Class="ManyJobs.PatchOps.IfWorkTypeIsEnabled">
    <workType>MJobs_Caring</workType>
    <operations>

      <li Class="PatchOperationReplace">
        <xpath>Defs/WorkGiverDef[defName = "AG_AdministerMetal"]/workType</xpath>
        <value>
          <workType>MJobs_Caring</workType>
        </value>
      </li>

      <li Class="PatchOperationReplace">
        <xpath>Defs/WorkGiverDef[defName = "AG_Warden_DeliverMetal"]/workType</xpath>
        <value>
          <workType>MJobs_Caring</workType>
        </value>
      </li>

    </operations>
  </Operation>

  <!-- #endregion -->


</Patch>
