<?xml version="1.0" encoding="utf-8" ?>
<Patch>

  <!-- #region MJobs_Drilling -->

  <Operation Class="XmlExtensions.OptionalPatchExternal">
    <ModClass>ManyJobs.ManyJobs</ModClass>
    <field>MJobs_Drilling</field>
    <caseTrue>

      <Operation Class="PatchOperationReplace">
        <xpath>Defs/WorkGiverDef[defName = "AB_CoreDrill"]/workType</xpath>
        <value>
          <workType>MJobs_Drilling</workType>
        </value>
      </Operation>

    </caseTrue>
  </Operation>

  <!-- #endregion -->

  <!-- #region MJobs_Smelting -->

  <Operation Class="XmlExtensions.OptionalPatchExternal">
    <ModClass>ManyJobs.ManyJobs</ModClass>
    <field>MJobs_Smelting</field>
    <caseTrue>

      <Operation Class="PatchOperationReplace">
        <xpath>Defs/WorkGiverDef[defName = "AB_DoBillsSmelter"]/workType</xpath>
        <value>
          <workType>MJobs_Smelting</workType>
        </value>
      </Operation>

    </caseTrue>
  </Operation>

  <!-- #endregion -->

</Patch>
