<?xml version="1.0" encoding="utf-8" ?>
<Patch>

  <!-- #region MJobs_PriorityHauling -->

  <Operation Class="ManyJobs.PatchOps.IfWorkTypeIsEnabled">
    <workType>MJobs_PriorityHauling</workType>
    <operations>

      <li Class="PatchOperationReplace">
        <xpath>Defs/WorkGiverDef[defName = "IP_InsertFirstIngredient"]/workType</xpath>
        <value>
          <workType>MJobs_PriorityHauling</workType>
        </value>
      </li>

      <li Class="PatchOperationReplace">
        <xpath>Defs/WorkGiverDef[defName = "IP_InsertSecondIngredient"]/workType</xpath>
        <value>
          <workType>MJobs_PriorityHauling</workType>
        </value>
      </li>

      <li Class="PatchOperationReplace">
        <xpath>Defs/WorkGiverDef[defName = "IP_InsertThirdIngredient"]/workType</xpath>
        <value>
          <workType>MJobs_PriorityHauling</workType>
        </value>
      </li>

      <li Class="PatchOperationReplace">
        <xpath>Defs/WorkGiverDef[defName = "IP_RemoveProduct"]/workType</xpath>
        <value>
          <workType>MJobs_PriorityHauling</workType>
        </value>
      </li>

    </operations>
  </Operation>

  <!-- #endregion -->

</Patch>
